{
    "name": "webcraftdg/fractal-cms-core",
    "description": "FractalCMS - CORE - un CMS léger et modulaire basé sur Yii2",
    "keywords": [
        "fractal-cms",
        "cms",
        "core",
        "php",
        "yii2",
        "website",
        "webcraftdg"
    ],
    "version": "v2.1.0",
    "homepage": "https://github.com/webcraftdg/fractal-cms-core",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/webcraftdg/fractal-cms-core/issues"
    },
    "authors": [
        {
            "name": "David Ghyse",
            "email": "davidg@webcraftdg.fr"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-intl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "yiisoft/yii2": "^2.0",
        "league/flysystem": "^3.25"
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "^2.0",
        "codeception/module-db": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "fractalCms\\core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": []
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "archive": {
        "exclude": ["tests"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
